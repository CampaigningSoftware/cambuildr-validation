{
    "name": "campaigningsoftware/cambuildr-validation",
    "description": "Validation rules that are used by the CamBuildr",
    "type": "library",
    "require": {
        "laravel/framework": "^11.0|^12.0",
        "php": "^8.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Schindler",
            "email": "stefan.schindler@cambuildr.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CampaigningSoftware\\CambuildrValidation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CampaigningSoftware\\CambuildrValidation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "extra": {
        "laravel": {
            "providers": [
                "CampaigningSoftware\\CambuildrValidation\\CambuildrValidationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^9.0|^10.0"
    }
}
